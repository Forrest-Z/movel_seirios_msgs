<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node pkg="diagnostic_aggregator" type="add_analyzers" name="add_analyzers">
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/default/new_analyzers.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/default/new_analyzers.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/$(optenv MOVEL_ROBOT_PROFILE default)/new_analyzers.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_USER)/robot/new_analyzers.yaml" unless="$(optenv MOVEL_DEV_MODE true)"/>
  </node>

</launch>
