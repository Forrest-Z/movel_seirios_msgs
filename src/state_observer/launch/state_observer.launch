<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <node pkg="state_observer" type="state_observer" name="state_observer">
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/default/state_observer.yaml"/>
    <!--rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/default/state_observer.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/$(optenv MOVEL_ROBOT_PROFILE default)/state_observer.yaml"/-->
    <rosparam file="$(env MOVEL_CONFIG_ROOT_USER)/robot/state_observer.yaml" unless="$(optenv MOVEL_DEV_MODE true)"/>
  </node>

</launch>
