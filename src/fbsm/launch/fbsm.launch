<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <node pkg="fbsm" type="line_extraction_node" name="fbsm">
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/default/fbsm.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/default/fbsm.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_DEFAULT)/robot/$(env MOVEL_ROBOT_BASE)/$(optenv MOVEL_ROBOT_PROFILE default)/fbsm.yaml"/>
    <rosparam file="$(env MOVEL_CONFIG_ROOT_USER)/robot/fbsm.yaml" unless="$(optenv MOVEL_DEV_MODE true)"/>
  </node>

</launch>
