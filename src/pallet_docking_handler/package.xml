<?xml version="1.0"?>
<package format="2">
  <name>pallet_docking_handler</name>
  <version>0.0.0</version>
  <description>The pallet_docking_handler package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
  <!-- Example:  -->
  <!-- <maintainer email="jane.doe@example.com">Jane Doe</maintainer> -->
  <maintainer email="contact@movel.ai">movel</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>task_supervisor</build_depend>
  <build_export_depend>pluginlib</build_export_depend>
  <build_export_depend>roscpp</build_export_depend>
  <build_export_depend>task_supervisor</build_export_depend>
  <exec_depend>pluginlib</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>task_supervisor</exec_depend>
  <depend>movel_seirios_msgs</depend>
  <depend>ros_utils</depend>
  <depend>sensor_msgs</depend>
  <depend>std_srvs</depend>
  <depend>geometry_msgs</depend>
  <depend>boost</depend>
  <depend>nav_msgs</depend>
  <depend>apriltag_ros</depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <task_supervisor plugin="${prefix}/pallet_docking_handler_plugins.xml" />
  </export>
</package>
