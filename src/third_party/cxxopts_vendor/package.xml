<?xml version="1.0"?>
<?xml-model
  href="http://download.ros.org/schema/package_format2.xsd"
  schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>cxxopts_vendor</name>
  <version>2.2.1</version>
  <description>
    Wrapper around cxxopts, providing nothing but a dependency on cxxopts, on some systems.
    On others, it provides an ExternalProject build of cxxopts.
  </description>
  <maintainer email="contact@movel.ai">Movel AI</maintainer>
  <license>Apache License 2.0</license>  <!-- the contents of this package are Apache 2.0 -->
  <license>MIT</license>  <!-- cxxopts is MIT -->

  <url type="website">https://github.com/jarro2783/cxxopts</url>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>cxxopts</depend>

</package>
